{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "$comment1": "use `npm run dev` to start App.svelte and index.html a development server",
    "build": "rollup -c",
    "dev": "rollup -c -w",

    "$comment2": "use `npm run dev2` to start Iro.svelte and iro.html a development server",
    "build2": "rollup -c rollup.config2.js",
    "dev2": "rollup -c rollup.config2.js -w",

    "$comment3": "use `npm run dev3` to start App.svelte(index.html) and Iro.svelte(iro.html) a development server",
    "build3": "rollup -c rollup.config.js & rollup -c rollup.config2.js",
    "dev3": "rollup -c rollup.config.js -w & rollup -c rollup.config2.js -w",

    "start": "sirv public --no-clear",
    "deploy": "npm run build && gh-pages -d public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "gh-pages": "^5.0.0",
    "rollup": "^3.15.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.2",
    "svelte": "^3.55.0"
  },
  "dependencies": {
    "better-sqlite3": "^8.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ramda": "^0.29.0",
    "sirv-cli": "^2.0.2",
    "validator": "^13.9.0"
  }
}
